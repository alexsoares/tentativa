<div id="interno1">
   <br/>
   <%= link_to 'Ambiente configurado', "#inline_ambiente", :class =>"line", :type => 1%>
   <hr/>
   <div id="atual">
     <% if @a.present? %>
      <%= render "configuracoes/show" %>
     <%end%>
     <br/>
     <% if session[:falha].present? %>
        <%= render :text => session[:falha] %>
     <%end%>
   </div>
   <hr/>
    <h1>EMPRÉSTIMOS EM VIGÊNCIA</h1>
    <b>Usuario corrente:</b> <%= link_to h(current_user.login.capitalize), edit_user_path(current_user)%><br/>
    <b>Usuario GPD: </b><%= current_user.unidade.unidades_gpd.nome %>
    <br/>

    <div id="emprestimos"><%= render "emprestimos" %></div>
</div>    
